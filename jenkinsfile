pipeline{
tools{
      jdk 'myjava'
      maven 'mymaven'
      }
      agent any
      stages{
      stage('git clone from webhookvuvuv'){
             steps{
                  git 'https://github.com/baburao7975/DevOpsAddressBook.git'
                  }
             }
      stage(package){
           steps{
               sh 'mvn package'
               }
            }
      }
   }
